<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IG_Type__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
    RecordType.Name = &quot;Receivable&quot;,
    IF(
        ISBLANK(GiftCommitmentId),
        &quot;Cash Giver&quot;,
