<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unqualified_Reason</fullName>
    <active>true</active>
    <description>Unqualified reason must not be blank if status is set to unqualified</description>
    <errorConditionFormula>AND(
    ISPICKVAL(Status, &quot;Unqualified&quot;),
    ISPICKVAL(Unqualified_Reason_sj__c, &quot;&quot;)
)</errorConditionFormula>
    <errorMessage>Please enter an unqualified reason</errorMessage>
</ValidationRule>
